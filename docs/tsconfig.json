{
  // Change this to match your project
  "extends": "./tsconfig.base.json",
  "include": [
    "src/**/*.ts",
    "../packages/*",
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "vue-fantable/*": [
        "../*"
      ]
    },
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
    ],
    "outDir": "types"
  }
}